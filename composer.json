{
    "name": "vairogs/vairogs",
    "description": "Vairogs",
    "type": "library",
    "keywords": [
        "auth",
        "components",
        "openid",
        "sitemap",
        "steam",
        "utils",
        "vairogs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dāvis Zālītis (k0d3r1s)",
            "email": "k0d3r1s@vairogs.com",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/k0d3r1s"
        },
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/k0d3r1s"
        },
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/vairogs/vairogs"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "doctrine/collections": "~1.6",
        "doctrine/dbal": "~2.8",
        "doctrine/orm": "~2.6",
        "doctrine/persistence": "~2.1",
        "monolog/monolog": "~2.1",
        "psr/http-message": "^1",
        "symfony/config": "~5.2",
        "symfony/console": "~5.2",
        "symfony/dependency-injection": "~5.2",
        "symfony/framework-bundle": "~5.2",
        "symfony/http-foundation": "~5.2",
        "symfony/http-kernel": "~5.2",
        "symfony/routing": "~5.2",
        "symfony/validator": "~5.2",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~5.2"
    },
    "replace": {
        "ravenflux/getenv": "self.version",
        "ravenflux/php-functions": "self.version",
        "ravenflux/ravenflux": "self.version",
        "ravenflux/sort-functions": "self.version",
        "vairogs/addon": "self.version",
        "vairogs/addon-auth": "self.version",
        "vairogs/addon-auth-openid": "self.version",
        "vairogs/addon-auth-openid-steam": "self.version",
        "vairogs/auth-openid": "self.version",
        "vairogs/auth": "self.version",
        "vairogs/components": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/utils": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/",
            "RavenFlux\\Twig\\": "src/RavenFlux/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
