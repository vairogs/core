{
    "name": "vairogs/vairogs",
    "description": "Vairogs",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "utils",
        "components",
        "auth",
        "openid"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.0"
    },
    "replace": {
        "vairogs/components": "self.version",
        "vairogs/utils": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/openid": "self.version",
        "ravenflux/ravenflux": "self.version",
        "ravenflux/sort-functions": "self.version",
        "ravenflux/php-functions": "self.version",
        "ravenflux/getenv": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/",
            "RavenFlux\\Twig\\": "src/RavenFlux/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
