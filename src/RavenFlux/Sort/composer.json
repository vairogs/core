{
    "name": "ravenflux/sort-functions",
    "description": "Twig extension that implements sorting functions in Twig template",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "ravenflux",
        "twig",
        "twig-extension",
        "symfony",
        "symfony2",
        "symfony3",
        "symfony4",
        "sort"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "twig/twig": "~1.27|~2.0",
        "twig/extensions": "~1.5",
        "doctrine/collections": "~1.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\RavenFlux\\Sort\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
