{
    "name": "ravenflux/ravenflux",
    "description": "RavenFlux Twig functions",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "ravenflux",
        "twig",
        "twig-extension",
        "symfony",
        "symfony2",
        "symfony3",
        "symfony4",
        "php",
        "sort",
        "env",
        "getenv"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "twig/twig": "3.*",
        "doctrine/collections": "~1.6",
        "vairogs/utils": "3.x-dev"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RavenFlux\\Twig\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "replace": {
        "ravenflux/getenv": "self.version",
        "ravenflux/php-functions": "self.version",
        "ravenflux/sort-functions": "self.version"
    }
}
