{
    "name": "vairogs/addon-auth-openid",
    "description": "Utils: Auth library: OpenID client models",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "auth",
        "openid",
        "symfony",
        "steam",
        "addon"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/orm": "~2.6",
        "vairogs/auth-openid": "3.x-dev"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Addon\\Auth\\OpenID\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "replace": {
        "vairogs/addon-auth-openid-steam": "self.version"
    }
}
