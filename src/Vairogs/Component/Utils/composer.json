{
    "name": "vairogs/utils",
    "description": "Symfony utility classes",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "utils",
        "symfony",
        "array",
        "date",
        "doctrine",
        "http-foundation",
        "http",
        "json",
        "manipulation",
        "object",
        "php",
        "prime",
        "psr",
        "response",
        "sort",
        "string",
        "text",
        "unique",
        "uri",
        "validation"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "monolog/monolog": "~2.1",
        "symfony/config": "~5.2",
        "symfony/dependency-injection": "~5.2",
        "symfony/http-foundation": "~5.2",
        "symfony/http-kernel": "~5.2",
        "doctrine/collections": "~1.6",
        "doctrine/dbal": "~2.8",
        "doctrine/orm": "~2.6",
        "doctrine/persistence": "~2.1",
        "psr/http-message": "^1",
        "twig/twig": "2.*|3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~5.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\Utils\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
