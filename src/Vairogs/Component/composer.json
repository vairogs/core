{
    "name": "vairogs/components",
    "description": "Vairogs components",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "utils",
        "symfony",
        "array",
        "date",
        "doctrine",
        "http-foundation",
        "http",
        "json",
        "manipulation",
        "object",
        "php",
        "prime",
        "psr",
        "response",
        "sort",
        "string",
        "text",
        "unique",
        "uri",
        "validation",
        "auth",
        "openid",
        "steam"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "doctrine/collections": "~1.6",
        "doctrine/dbal": "~2.8",
        "doctrine/orm": "~2.6",
        "psr/http-message": "^1",
        "symfony/config": "~5.0|~5.1",
        "symfony/console": "~5.0|~5.1",
        "symfony/dependency-injection": "~5.0|~5.1",
        "symfony/framework-bundle": "~5.0|~5.1",
        "symfony/http-foundation": "~5.0|~5.1",
        "symfony/http-kernel": "~5.0|~5.1",
        "symfony/routing": "~5.0|~5.1",
        "symfony/validator": "~5.0|~5.1",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~5.0|~5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "replace": {
        "vairogs/utils": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/auth": "self.version",
        "vairogs/auth-openid": "self.version"
    }
}
