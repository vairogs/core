    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "doctrine/collections": "~1.6",
        "doctrine/dbal": "~2.8",
        "doctrine/orm": "~2.6",
        "doctrine/persistence": "~2.1",
        "monolog/monolog": "~2.1",
        "psr/http-message": "^1",
        "symfony/config": "~5.2",
        "symfony/console": "~5.2",
        "symfony/dependency-injection": "~5.2",
        "symfony/framework-bundle": "~5.2",
        "symfony/http-foundation": "~5.2",
        "symfony/http-kernel": "~5.2",
        "symfony/routing": "~5.2",
        "symfony/validator": "~5.2",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~5.2"
    },
    "replace": {
        "vairogs/auth": "self.version",
        "vairogs/auth-openid": "self.version",
        "vairogs/cache": "self.version",
        "vairogs/cache-utils": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/utils": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
