{
    "name": "vairogs/sitemap",
    "description": "Utils: Sitemap library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "sitemap",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-zlib": "*",
        "symfony/dependency-injection": "~5.2",
        "symfony/http-kernel": "~5.2",
        "symfony/config": "~5.2",
        "symfony/http-foundation": "~5.2",
        "symfony/console": "~5.2",
        "symfony/routing": "~5.2",
        "symfony/validator": "~5.2",
        "symfony/framework-bundle": "~5.2",
        "vairogs/utils": "3.x-dev"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\Sitemap\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
